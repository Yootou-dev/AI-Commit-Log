{
  "version": "0.1.14",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "clog-ai": "bin/clog-ai"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "clog-ai",
  "author": "yutou",
  "description": "Generate commit log with AI",
  "keywords": [
    "commit",
    "log",
    "ai",
    "clog",
    "gpt"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/aoao-eth/AI-Commit-Log"
  },
  "module": "dist/clog-ai.esm.js",
  "size-limit": [
    {
      "path": "dist/clog-ai.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/clog-ai.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "babel-plugin-inline-import": "^3.0.0",
    "execa": "^8.0.1",
    "loading-cli": "^1.1.2",
    "prettier": "^3.1.1",
    "tsdx": "^0.14.1"
  }
}
